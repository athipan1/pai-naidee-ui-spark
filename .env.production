# Production Environment Configuration
VITE_APP_TITLE=PaiNaiDee - Thailand Travel Guide
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=production

# API Configuration - Update with production endpoints
VITE_API_BASE_URL=https://api.painaidee.com/api

# Feature Flags - Production optimized
VITE_ENABLE_DEBUG=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_TESTING_FEATURES=false

# PWA Configuration
VITE_PWA_ENABLED=true

# Performance & Monitoring
VITE_PERFORMANCE_MONITORING=true

# Search & Vector Configuration
VITE_VECTOR_ENABLE=true
VITE_VECTOR_DIM=384
VITE_SEARCH_DEFAULT_MODE=hybrid
VITE_RECENCY_HALF_LIFE_DAYS=7
VITE_RECOMMENDATIONS_ENABLE=true

# Security
VITE_SECURE_MODE=true