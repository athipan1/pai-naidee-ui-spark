
# API Configuration
VITE_API_BASE_URL=http://localhost:5000/api

# Application Configuration
VITE_APP_VERSION=1.0.0
VITE_APP_NAME=PaiNaiDee

# Development Features (set to "true" to enable)
VITE_ENABLE_DEBUG=false
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_TESTING_FEATURES=false

# Environment
VITE_ENVIRONMENT=production

# Map Configuration (if using external map services)
# VITE_MAP_API_KEY=your_map_api_key_here

# Analytics (if using external analytics)
# VITE_ANALYTICS_ID=your_analytics_id_here

# Social Login (if implemented)
# VITE_GOOGLE_CLIENT_ID=your_google_client_id
# VITE_FACEBOOK_APP_ID=your_facebook_app_id

# CDN Configuration (if using external CDN)
# VITE_CDN_BASE_URL=https://your-cdn-domain.com

# Feature Flags
VITE_ENABLE_PWA=true
VITE_ENABLE_OFFLINE=true
